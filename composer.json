{
    "name": "gusnips/craft-elasticsearch",
    "description": "Bring the power of Elasticsearch to your Craft CMS projects.",
    "type": "craft-plugin",
    "version": "1.2.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "elasticsearch"
    ],
    "license": "",
    "authors": [
        {
            "name": "Gustavo Salome",
            "homepage": "https://github.com/gusnips"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "la-haute-societe/yii2-elasticsearch": "2.1.0",
        "guzzlehttp/guzzle": "~6.3.3"
    },
    "autoload": {
        "psr-4": {
            "lhs\\elasticsearch\\": "src/"
        }
    },
    "extra": {
        "name": "Elasticsearch",
        "handle": "elasticsearch",
        "schemaVersion": "1.2.2",
        "components": {
            "elasticsearch": "lhs\\elasticsearch\\services\\Elasticsearch"
        },
        "class": "lhs\\elasticsearch\\Elasticsearch"
    }
}
